<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:customNS="http://schemas.android.com/apk/res/net.one97.paytm" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/sim_changed_nav_graph" app:startDestination="@+id/selectIssueFragment">
    <fragment android:name="net.one97.paytm.oauth.fragment.SelectIssueFragment" android:id="@+id/selectIssueFragment">
        <action android:id="@+id/navActionEmailEnterOtp" app:destination="@+id/verifyEmailPhoneOtpFragment" app:enterAnim="@animator/slide_in_left" app:exitAnim="@animator/slide_out_left" app:popEnterAnim="@animator/slide_in_right" app:popExitAnim="@animator/nav_slide_out_right"/>
