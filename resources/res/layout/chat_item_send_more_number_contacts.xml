<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="55dp">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="55dp">
        <ImageView android:id="@+id/img_phone_icon" android:layout_width="16dp" android:layout_height="16dp" android:layout_marginLeft="20dp" android:src="@mipmap/chat_icon_send_more_number_contacts" android:layout_centerVertical="true" android:layout_marginStart="20dp"/>
        <CheckBox android:id="@+id/rb_item_send_selected" android:background="@drawable/chat_shape_checkbox_style" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="20dp" android:button="@null" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:layout_marginEnd="20dp" android:layout_alignParentEnd="true"/>
        <TextView android:textSize="14sp" android:textColor="@color/black" android:gravity="center_vertical" android:id="@+id/tv_item_send_phone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_toRightOf="@+id/cl_pic_bg" android:layout_centerVertical="true" android:layout_marginStart="20dp" android:layout_toEndOf="@+id/img_phone_icon"/>
    </RelativeLayout>
</FrameLayout>
