<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@color/white" android:padding="@dimen/dimen_20dp" android:layout_width="match_parent" android:layout_height="wrap_content">
    <com.paytm.utility.RoboTextView android:textSize="13sp" android:textColor="@color/color_666666" android:id="@+id/dialogTitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="5dp" android:text="@string/tracking_details" android:textAllCaps="true" app:fontType="3"/>
    <androidx.appcompat.widget.AppCompatImageView android:id="@+id/crossButton" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignParentRight="true" app:srcCompat="@drawable/ic_event_close"/>
    <com.paytm.utility.RoboTextView android:textSize="14sp" android:textColor="@color/black" android:id="@+id/id_text" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="30dp" android:layout_marginBottom="30dp" android:layout_below="@+id/crossButton" app:fontType="3"/>
    <com.paytm.utility.RoboTextView android:textSize="16sp" android:textColor="@color/paytm_blue" android:gravity="center" android:id="@+id/id_awb_button" android:background="@drawable/item_action_button_border" android:layout_width="match_parent" android:layout_height="45dp" android:text="Copy AWB Number" android:layout_below="@+id/id_text" app:fontType="3"/>
</RelativeLayout>
