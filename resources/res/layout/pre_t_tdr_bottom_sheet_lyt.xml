<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:customNS="http://schemas.android.com/apk/res/net.one97.paytm" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content">
        <RelativeLayout android:background="@drawable/pre_t_train_date_alert_farg_bg" android:paddingTop="@dimen/dimen_25dp" android:paddingBottom="@dimen/dimen_18dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/detail_close" android:layout_alignParentBottom="true">
            <RelativeLayout android:gravity="center_vertical" android:id="@+id/header_layout" android:paddingLeft="@dimen/dimen_15dp" android:paddingRight="@dimen/dimen_15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
                <com.paytm.utility.RoboTextView android:textSize="17sp" android:textStyle="normal" android:textColor="#000000" android:id="@+id/rules_header" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_below="@+id/parent_date" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:fontFamily="sans-serif-medium"/>
                <ImageView android:id="@+id/detail_close" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/pre_t_ic_close_minkyc" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View android:id="@+id/sep_head" android:background="@color/color_efefef" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="@dimen/dimen_15dp" android:layout_marginTop="22dp" android:layout_marginRight="@dimen/dimen_15dp" android:layout_below="@+id/header_layout"/>
            <com.paytm.utility.RoboTextView android:textSize="12sp" android:textStyle="normal" android:textColor="#666666" android:id="@+id/disclimer_txt" android:paddingLeft="@dimen/dimen_15dp" android:paddingRight="@dimen/dimen_15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_21dp" android:layout_below="@+id/sep_head" android:lineSpacingExtra="5.5sp" android:fontFamily="sans-serif"/>
            <View android:id="@+id/sep_head_2" android:background="@color/color_efefef" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="@dimen/dimen_15dp" android:layout_marginRight="@dimen/dimen_15dp" android:layout_below="@+id/disclimer_txt"/>
            <com.paytm.utility.RoboTextView android:textSize="11sp" android:textStyle="normal" android:textColor="#666666" android:id="@+id/refund_rules" android:paddingLeft="@dimen/dimen_15dp" android:paddingRight="@dimen/dimen_15dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_24dp" android:layout_below="@+id/sep_head_2" android:lineSpacingExtra="5dp" android:fontFamily="sans-serif"/>
        </RelativeLayout>
    </RelativeLayout>
</FrameLayout>
