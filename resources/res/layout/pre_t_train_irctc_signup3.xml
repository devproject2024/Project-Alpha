            <ImageView android:layout_gravity="center" android:id="@+id/office_address_clear" android:visibility="gone" android:clickable="true" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_10dp" android:layout_marginRight="15dp" android:src="@drawable/pre_t_cross_grey"/>
        </LinearLayout>
        <View android:id="@+id/office_address_view" android:background="@color/payment_success_line_grey" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="15dp" android:layout_marginTop="13dp" android:layout_marginRight="15dp"/>
        <com.paytm.utility.RoboTextView android:textColor="@color/cart_oos_text" android:id="@+id/office_address_error" android:paddingLeft="15dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="2dp" app:fontType="3"/>
    </LinearLayout>
    <Button android:textSize="18sp" android:textColor="@color/white" android:id="@+id/irctc_singup_btn" android:background="@drawable/pre_t_train_rounded_blue_button" android:layout_width="match_parent" android:layout_height="@dimen/dimen_50dp" android:layout_marginLeft="@dimen/dimen_15dp" android:layout_marginTop="@dimen/dimen_40dp" android:layout_marginRight="@dimen/dimen_15dp" android:layout_marginBottom="@dimen/dimen_40dp" android:text="@string/sign_up" android:textAllCaps="false" android:stateListAnimator="@null"/>
