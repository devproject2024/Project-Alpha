<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/nsv_main_content" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <ScrollView android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
            <ImageView android:id="@+id/iv_back_arrow" android:background="?android:attr/selectableItemBackground" android:paddingTop="8dp" android:paddingRight="8dp" android:paddingBottom="8dp" android:layout_width="@dimen/dimen_32dp" android:layout_height="@dimen/dimen_32dp" android:layout_marginLeft="12dp" android:layout_marginTop="15dp" android:src="@drawable/pb_ic_back"/>
            <RelativeLayout android:id="@+id/rl_share_wrapper" android:background="@drawable/white_rounded18_bg" android:layout_width="36dp" android:layout_height="36dp" android:layout_marginTop="15dp" android:layout_marginRight="@dimen/dimen_10dp" android:layout_alignParentRight="true">
                <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/pb_ic_share_black" android:layout_centerInParent="true"/>
            </RelativeLayout>
            <ImageView android:id="@+id/center_logo" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="15dp" android:src="@drawable/pb_ic_payments_bank_logo" android:layout_centerHorizontal="true"/>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@+id/ll_cash_heading_wrapper" android:paddingTop="6dp" android:paddingBottom="6dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:layout_below="@+id/center_logo">
                <TextView android:textSize="15sp" android:textStyle="normal" android:textColor="#ffffff" android:gravity="center_horizontal" android:id="@+id/top_title_tv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:lineSpacingExtra="13sp" android:fontFamily="sans-serif-medium"/>
                <ImageView android:id="@+id/top_icon_iv" android:layout_width="17dp" android:layout_height="17dp" android:layout_marginLeft="@dimen/dimen_16dp" android:src="@drawable/cash_withdraw_success"/>
            </LinearLayout>
            <TextView android:textSize="15sp" android:textStyle="bold" android:textColor="#1d252d" android:id="@+id/titleTv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="32dp" android:layout_marginRight="100dp" android:text="Please collect cash from" android:layout_below="@+id/ll_cash_heading_wrapper" android:lineSpacingExtra="5sp" android:fontFamily="sans-serif"/>
            <ImageView android:id="@+id/success_icon_iv" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_20dp" android:layout_marginRight="@dimen/dimen_16dp" android:src="@drawable/bank_cash_withdraw_ic_collect_cash" android:layout_below="@+id/ll_cash_heading_wrapper" android:layout_alignParentRight="true"/>
            <TextView android:textSize="24sp" android:textStyle="bold" android:textColor="#1d252d" android:id="@+id/tv_amnt" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="11dp" android:layout_below="@+id/titleTv" android:fontFamily="sans-serif" android:letterSpacing="0.01"/>
            <TextView android:textSize="12sp" android:textStyle="normal" android:textColor="#506d85" android:id="@+id/tv_amount_in_rupees" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_marginTop="7dp" android:layout_below="@+id/tv_amnt" android:fontFamily="sans-serif"/>
            <View android:id="@+id/rupeebottomLine" android:background="#f5f8fa" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="20dp" android:layout_marginTop="23dp" android:layout_below="@+id/tv_amount_in_rupees"/>
            <RelativeLayout android:id="@+id/success_from_lyt" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="23dp" android:layout_below="@+id/rupeebottomLine">
                <TextView android:textSize="12sp" android:textStyle="normal" android:textColor="#506d85" android:id="@+id/tv_cash_withdrawn_from" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:text="@string/pb_cw_from" android:textAllCaps="true" android:fontFamily="sans-serif" android:letterSpacing="0.08"/>
                <TextView android:textSize="15sp" android:textStyle="normal" android:textColor="#1d252d" android:id="@+id/tv_paytm_payments_bank" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_marginTop="8dp" android:text="@string/paytm_payments_bank" android:layout_below="@+id/tv_cash_withdrawn_from" android:fontFamily="sans-serif-medium"/>
                <RelativeLayout android:background="@drawable/pb_upi_circle_border_e2ebee" android:layout_width="50dp" android:layout_height="50dp" android:layout_marginRight="40dp" android:layout_alignParentRight="true">
                    <net.one97.paytm.common.widgets.CircularImageView android:id="@+id/paytm_circular" android:layout_width="40dp" android:layout_height="40dp" android:src="@drawable/ic_paytm_logo" android:layout_centerInParent="true"/>
                </RelativeLayout>
            </RelativeLayout>
            <TextView android:textSize="14sp" android:textStyle="normal" android:textColor="#ffffff" android:gravity="center_horizontal" android:id="@+id/retry_button" android:background="@color/paytm_blue" android:paddingLeft="18dp" android:paddingTop="14dp" android:paddingRight="18dp" android:paddingBottom="14dp" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_marginTop="20dp" android:text="Retry" android:singleLine="true" android:layout_below="@+id/success_from_lyt" android:fontFamily="sans-serif-medium" android:paddingStart="42dp" android:paddingEnd="42dp"/>
            <TextView android:textSize="12sp" android:textStyle="normal" android:textColor="#506d85" android:id="@+id/transaction_id" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_marginTop="32dp" android:text="@string/pb_transaction_id" android:layout_below="@+id/retry_button" android:fontFamily="sans-serif"/>
            <TextView android:textSize="12sp" android:textStyle="normal" android:textColor="#506d85" android:id="@+id/tv_date" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_marginTop="8dp" android:text="12:44 pm, 10 Feb 2019" android:layout_below="@+id/transaction_id" android:fontFamily="sans-serif"/>
            <LinearLayout android:orientation="horizontal" android:id="@+id/v_sep" android:background="#f3f7f8" android:layout_width="match_parent" android:layout_height="10dp" android:layout_marginTop="40dp" android:layout_below="@+id/tv_date"/>
            <TextView android:textSize="15sp" android:textStyle="normal" android:textColor="#1d252d" android:id="@+id/tv_updated_saving_account" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_marginTop="@dimen/dimen_20dp" android:layout_below="@+id/v_sep" android:fontFamily="sans-serif"/>
            <View android:id="@+id/sep_1_1" android:background="#f3f7f8" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_marginTop="@dimen/dimen_20dp" android:layout_below="@+id/tv_updated_saving_account"/>
            <RelativeLayout android:orientation="horizontal" android:id="@+id/ll_generate_qr" android:paddingTop="15dp" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_below="@+id/sep_1_1">
                <ImageView android:id="@+id/img_1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/cash_withdraw_ic_scan_qr" android:layout_centerVertical="true"/>
                <TextView android:textSize="15sp" android:textStyle="normal" android:textColor="#1d252d" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:text="@string/pb_cw_qr_generate_menu" android:layout_toRightOf="@+id/img_1" android:fontFamily="sans-serif"/>
                <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="17dp" android:src="@drawable/right_arrow" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View android:id="@+id/sep_1_2" android:background="#f3f7f8" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_below="@+id/ll_generate_qr"/>
            <RelativeLayout android:orientation="horizontal" android:id="@+id/ll_view_passbook" android:paddingTop="15dp" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_below="@+id/sep_1_2">
                <ImageView android:id="@+id/img_2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/cash_withdraw_passbook" android:layout_centerVertical="true"/>
                <TextView android:textSize="15sp" android:textStyle="normal" android:textColor="#1d252d" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:text="@string/pb_cw_view_passbook_menu" android:layout_toRightOf="@+id/img_2" android:fontFamily="sans-serif"/>
                <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="17dp" android:src="@drawable/right_arrow" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View android:id="@+id/sep_1_3" android:background="#f3f7f8" android:layout_width="match_parent" android:layout_height="1dp" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_below="@+id/ll_view_passbook"/>
            <RelativeLayout android:orientation="horizontal" android:id="@+id/ll_view_help" android:paddingTop="15dp" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:layout_below="@+id/sep_1_3">
                <ImageView android:id="@+id/img_3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/cash_withdraw_ic_help" android:layout_centerVertical="true"/>
                <TextView android:textSize="15sp" android:textStyle="normal" android:textColor="#1d252d" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_20dp" android:text="@string/bank_contact_us" android:layout_toRightOf="@+id/img_3" android:fontFamily="sans-serif"/>
                <ImageView android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="17dp" android:src="@drawable/right_arrow" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
            </RelativeLayout>
            <View android:orientation="horizontal" android:id="@+id/borderrr" android:background="#f3f7f8" android:layout_width="match_parent" android:layout_height="10dp" android:layout_below="@+id/ll_view_help"/>
            <RelativeLayout android:id="@+id/bottom_part" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/borderrr">
                <ImageView android:id="@+id/stamp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="50dp" android:layout_marginTop="40dp" android:layout_marginBottom="50dp" android:src="@drawable/cash_withdraw_stamp_copy_9"/>
                <TextView android:textSize="12sp" android:textStyle="normal" android:textColor="#666666" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_16dp" android:layout_marginTop="40dp" android:text="@string/fd_status_trust_text" android:layout_toRightOf="@+id/stamp" android:lineSpacingExtra="4sp" android:fontFamily="sans-serif-light" android:letterSpacing="0.04"/>
                <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/stamp">
                    <View android:background="@color/color_00b9f5" android:layout_width="match_parent" android:layout_height="8dp"/>
                    <View android:background="@color/color_012b72" android:layout_width="match_parent" android:layout_height="8dp"/>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
    <RelativeLayout android:id="@+id/rl_loader_wrapper" android:background="@color/white" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
        <ProgressBar android:layout_gravity="top|center_vertical|center_horizontal|center" android:id="@+id/pb_progress" android:background="@android:color/transparent" android:visibility="visible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_100dp" android:indeterminateDrawable="@drawable/pb_pb_paytm_progress" android:layout_centerInParent="true"/>
    </RelativeLayout>
</RelativeLayout>
