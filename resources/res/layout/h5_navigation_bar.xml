<?xml version="1.0" encoding="utf-8"?>
<com.alipay.mobile.nebula.view.H5TitleBarFrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/h5_title_bar" android:background="@android:color/transparent" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="vertical" android:id="@+id/h5_rl_title_bar" android:background="@android:color/transparent" android:layout_width="match_parent" android:layout_height="wrap_content">
        <View android:id="@+id/h5_status_bar_adjust_view" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <ViewStub android:orientation="vertical" android:id="@+id/h5_full_search_bar_stub" android:background="@android:color/transparent" android:visibility="gone" android:layout="@layout/h5_empty_linearlayout" android:inflatedId="@+id/h5_full_search_bar" android:layout_width="match_parent" android:layout_height="@dimen/h5_title_height"/>
        <RelativeLayout android:id="@+id/h5_title_bar_layout" android:layout_width="match_parent" android:layout_height="@dimen/h5_title_height">
            <ImageButton android:id="@+id/h5_tv_nav_back" android:background="@android:color/transparent" android:layout_width="48dp" android:layout_height="match_parent" android:src="@drawable/h5_title_bar_back_btn_selector" android:scaleType="centerInside" android:layout_alignParentLeft="true" android:layout_centerVertical="true" android:contentDescription="@string/h5_backward" android:layout_alignParentStart="true"/>
            <ImageButton android:id="@+id/h5_nav_close" android:background="@android:color/transparent" android:clickable="true" android:layout_width="48dp" android:layout_height="match_parent" android:layout_marginLeft="-6dp" android:src="@drawable/h5_title_bar_close_btn_selector" android:scaleType="centerInside" android:layout_toRightOf="@+id/h5_tv_nav_back" android:layout_centerVertical="true" android:contentDescription="@string/h5_close" android:layout_marginStart="-6dp" android:layout_toEndOf="@+id/h5_tv_nav_back"/>
            <View android:layout_gravity="center" android:id="@+id/h5_v_divider" android:background="@color/h5_nav_bar_divider" android:layout_width="0.7dp" android:layout_height="24dp" android:layout_marginRight="12dp" android:layout_toRightOf="@+id/h5_nav_close" android:layout_centerVertical="true" android:layout_marginEnd="12dp" android:layout_toEndOf="@+id/h5_nav_close"/>
            <RelativeLayout android:orientation="horizontal" android:id="@+id/h5_rl_title" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_toLeftOf="@+id/h5_nav_options1" android:layout_toRightOf="@+id/h5_v_divider" android:layout_centerVertical="true" android:layout_toStartOf="@+id/h5_nav_options1" android:layout_toEndOf="@+id/h5_v_divider">
                <ViewStub android:gravity="left|center_vertical|center_horizontal|center" android:orientation="horizontal" android:id="@+id/h5_embed_title_search_stub" android:visibility="gone" android:layout="@layout/h5_empty_linearlayout" android:inflatedId="@+id/h5_embed_title_search" android:layout_width="300dp" android:layout_height="@dimen/h5_title_height"/>
                <LinearLayout android:gravity="left|center_vertical|center_horizontal|center" android:orientation="vertical" android:id="@+id/h5_ll_title" android:layout_width="wrap_content" android:layout_height="match_parent">
                    <FrameLayout android:layout_width="wrap_content" android:layout_height="wrap_content">
                        <TextView android:textSize="@dimen/h5_nav_title_text" android:textColor="@color/h5_white" android:ellipsize="end" android:gravity="left" android:id="@+id/h5_tv_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true"/>
                        <ImageView android:id="@+id/h5_tv_title_img" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="36dp" android:scaleType="centerInside"/>
                    </FrameLayout>
                    <TextView android:textSize="@dimen/h5_nav_subtitle_text" android:textColor="@color/h5_white" android:gravity="left" android:id="@+id/h5_tv_subtitle" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:singleLine="true"/>
                </LinearLayout>
                <ViewStub android:id="@+id/h5_nav_loading_stub" android:layout="@layout/h5_nav_loading" android:inflatedId="@+id/h5_nav_loading" android:layout_width="17dp" android:layout_height="17dp" android:layout_marginLeft="10dp" android:layout_toRightOf="@+id/h5_ll_title" android:layout_centerVertical="true" android:layout_marginStart="10dp" android:layout_toEndOf="@+id/h5_ll_title"/>
            </RelativeLayout>
            <FrameLayout android:id="@+id/h5_nav_options" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="6dp" android:layout_alignParentRight="true" android:layout_centerVertical="true" android:layout_marginStart="6dp" android:layout_alignParentEnd="true">
                <ImageButton android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/h5_bt_image" android:background="@color/h5_transparent" android:padding="12dp" android:visibility="gone" android:layout_width="48dp" android:layout_height="48dp" android:scaleType="fitCenter" android:contentDescription="@string/h5_more"/>
                <TextView android:textSize="@dimen/h5_nav_button_text" android:textColor="#108ee9" android:ellipsize="end" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/h5_bt_text" android:background="@null" android:paddingRight="12dp" android:layout_width="wrap_content" android:layout_height="match_parent" android:singleLine="true" android:maxLength="8"/>
                <ImageButton android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/h5_bt_options" android:background="@null" android:padding="@dimen/h5_nav_options_selector_padding" android:visibility="gone" android:layout_width="@dimen/h5_nav_options_selector_width" android:layout_height="44dp" android:layout_marginRight="@dimen/h5_nav_options_selector_margin_right" android:src="@drawable/h5_title_bar_more_btn_selector" android:scaleType="centerInside" android:contentDescription="@string/h5_more" android:layout_marginEnd="@dimen/h5_nav_options_selector_margin_right"/>
                <FrameLayout android:layout_gravity="top|right|center_vertical|center_horizontal|center" android:id="@+id/h5_bt_dot" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="40dp" android:layout_marginRight="6dp" android:layout_marginEnd="6dp">
                    <ImageView android:id="@+id/h5_bt_dot_bg" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:src="@drawable/h5_redpoint" android:scaleType="centerInside"/>
                    <TextView android:textSize="12sp" android:textColor="#ffffff" android:gravity="center" android:id="@+id/h5_bt_dot_number" android:background="@drawable/h5_msg_flag_bg" android:layout_width="wrap_content" android:layout_height="wrap_content" android:includeFontPadding="false"/>
                </FrameLayout>
            </FrameLayout>
            <FrameLayout android:id="@+id/h5_nav_options1" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_marginLeft="6dp" android:layout_toLeftOf="@+id/h5_nav_options" android:layout_centerVertical="true" android:layout_marginStart="6dp" android:layout_toStartOf="@+id/h5_nav_options">
                <ImageButton android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/h5_bt_image1" android:background="@color/h5_transparent" android:padding="12dp" android:visibility="gone" android:layout_width="48dp" android:layout_height="48dp" android:scaleType="fitCenter"/>
                <TextView android:textSize="@dimen/h5_nav_button_text" android:textColor="#108ee9" android:ellipsize="end" android:gravity="center" android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/h5_bt_text1" android:background="@null" android:paddingRight="12dp" android:layout_width="wrap_content" android:layout_height="match_parent" android:singleLine="true" android:maxLength="8"/>
                <ImageButton android:layout_gravity="right|center_vertical|center_horizontal|center" android:id="@+id/h5_bt_options1" android:background="@null" android:padding="@dimen/h5_nav_options_selector_padding" android:visibility="gone" android:layout_width="@dimen/h5_nav_options_selector_width" android:layout_height="44dp" android:layout_marginRight="@dimen/h5_nav_options_selector_margin_right" android:src="@drawable/h5_title_bar_more_btn_selector" android:scaleType="centerInside" android:contentDescription="@string/h5_more" android:layout_marginEnd="@dimen/h5_nav_options_selector_margin_right"/>
                <FrameLayout android:layout_gravity="top|right|center_vertical|center_horizontal|center" android:id="@+id/h5_bt_dot1" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="40dp" android:layout_marginRight="6dp" android:layout_marginEnd="6dp">
                    <ImageView android:id="@+id/h5_bt_dot_bg1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6dp" android:src="@drawable/h5_point_small" android:scaleType="centerInside"/>
                    <TextView android:textSize="12sp" android:textColor="#ffffff" android:gravity="center" android:id="@+id/h5_bt_dot_number1" android:background="@drawable/h5_point_large" android:layout_width="wrap_content" android:layout_height="wrap_content" android:includeFontPadding="false"/>
                </FrameLayout>
            </FrameLayout>
        </RelativeLayout>
        <View android:id="@+id/h5_h_divider_intitle" android:background="@color/h5_nav_bar_bottomline" android:visibility="gone" android:layout_width="match_parent" android:layout_height="1px"/>
        <RelativeLayout android:id="@+id/adView" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <RelativeLayout android:id="@+id/h5_custom_view" android:layout_width="match_parent" android:layout_height="wrap_content"/>
    </LinearLayout>
</com.alipay.mobile.nebula.view.H5TitleBarFrameLayout>
