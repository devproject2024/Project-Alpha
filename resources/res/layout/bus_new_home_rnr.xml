<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@drawable/travel_res_bus_round_corners_white_rad_12dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content">
        <RelativeLayout android:id="@+id/rl_rating_star" android:padding="@dimen/dimen_16dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_10sp">
            <com.paytm.utility.RoboTextView android:textSize="@dimen/dimen_16sp" android:textStyle="bold" android:textColor="#1d252d" android:id="@+id/rating_text1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_2dp" android:text="@string/how_was_journey" android:layout_centerHorizontal="true"/>
            <com.paytm.utility.RoboTextView android:textSize="@dimen/dimen_12sp" android:textStyle="normal" android:textColor="#506d85" android:id="@+id/rating_text2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_2dp" android:text="@string/your_rating_valuable" android:layout_below="@+id/rating_text1" android:layout_centerHorizontal="true"/>
            <androidx.appcompat.widget.AppCompatImageView android:id="@+id/close_button" android:layout_width="@dimen/dimen_30dp" android:layout_height="@dimen/dimen_30dp" android:layout_marginTop="@dimen/dimen_5dp" android:layout_alignParentEnd="true" app:srcCompat="@drawable/bus_close_circle"/>
            <LinearLayout android:id="@+id/li_stars" android:paddingBottom="@dimen/dimen_12dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_25dp" android:layout_below="@+id/close_button" android:layout_centerHorizontal="true">
                <androidx.appcompat.widget.AppCompatImageView android:id="@+id/rating1" android:background="@drawable/bus_rating_star_normal" android:focusable="true" android:clickable="true" android:layout_width="@dimen/dimen_35dp" android:layout_height="@dimen/dimen_35dp"/>
                <androidx.appcompat.widget.AppCompatImageView android:id="@+id/rating2" android:background="@drawable/bus_rating_star_normal" android:focusable="true" android:clickable="true" android:layout_width="@dimen/dimen_35dp" android:layout_height="@dimen/dimen_35dp" android:layout_marginStart="@dimen/dimen_20dp"/>
                <androidx.appcompat.widget.AppCompatImageView android:id="@+id/rating3" android:background="@drawable/bus_rating_star_normal" android:focusable="true" android:clickable="true" android:layout_width="@dimen/dimen_35dp" android:layout_height="@dimen/dimen_35dp" android:layout_marginStart="@dimen/dimen_20dp"/>
                <androidx.appcompat.widget.AppCompatImageView android:id="@+id/rating4" android:background="@drawable/bus_rating_star_normal" android:focusable="true" android:clickable="true" android:layout_width="@dimen/dimen_35dp" android:layout_height="@dimen/dimen_35dp" android:layout_marginStart="@dimen/dimen_20dp"/>
                <androidx.appcompat.widget.AppCompatImageView android:id="@+id/rating5" android:background="@drawable/bus_rating_star_normal" android:focusable="true" android:clickable="true" android:layout_width="@dimen/dimen_35dp" android:layout_height="@dimen/dimen_35dp" android:layout_marginStart="@dimen/dimen_20dp"/>
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/rl_rating_star">
            <androidx.appcompat.widget.AppCompatImageView android:id="@+id/bus_image" android:layout_width="@dimen/dimen_56dp" android:layout_height="@dimen/dimen_56dp" android:layout_marginLeft="@dimen/dimen_30dp" android:layout_marginTop="@dimen/dimen_30dp" android:layout_marginRight="@dimen/dimen_20dp"/>
            <TextView android:textSize="12sp" android:textStyle="normal" android:textColor="#506d85" android:id="@+id/txt_operator_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_26dp" android:fontFamily="sans-serif-light" android:layout_toEndOf="@+id/bus_image"/>
            <TextView android:textSize="@dimen/dimen_14sp" android:textStyle="normal" android:textColor="#1b252e" android:id="@+id/city_src" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_3dp" android:layout_below="@+id/txt_operator_name" android:fontFamily="sans-serif-medium" android:layout_toEndOf="@+id/bus_image"/>
            <androidx.appcompat.widget.AppCompatImageView android:id="@+id/arrow_image" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="13dp" android:layout_below="@+id/txt_operator_name" android:layout_marginStart="5dp" android:layout_toEndOf="@+id/city_src"/>
            <TextView android:textSize="@dimen/dimen_14sp" android:textStyle="normal" android:textColor="#1b252e" android:id="@+id/city_des" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_3dp" android:layout_below="@+id/txt_operator_name" android:fontFamily="sans-serif-medium" android:layout_marginStart="5dp" android:layout_toEndOf="@+id/arrow_image"/>
            <TextView android:textSize="12sp" android:textStyle="normal" android:textColor="#506d85" android:id="@+id/travel_date" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dimen_2dp" android:layout_marginBottom="@dimen/dimen_50dp" android:layout_below="@+id/city_src" android:fontFamily="sans-serif-light" android:layout_toEndOf="@+id/bus_image"/>
        </RelativeLayout>
    </RelativeLayout>
</LinearLayout>
