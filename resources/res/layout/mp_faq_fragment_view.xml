<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:customNS="http://schemas.android.com/apk/res/net.one97.paytm" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/toolbar_rel_lay" android:paddingTop="@dimen/dimen_10dp" android:paddingBottom="@dimen/dimen_10dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <ImageButton android:id="@+id/back_button" android:background="@android:color/transparent" android:padding="8dp" android:paddingRight="15dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_14dp" android:src="@drawable/back_arrow"/>
        <TextView android:textSize="15sp" android:textStyle="bold" android:textColor="#000000" android:id="@+id/show_pass_in_text" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/dimen_25dp" android:text="@string/showing_movie_pass_in" android:layout_toRightOf="@+id/back_button" android:layout_centerVertical="true" android:fontFamily="sans-serif"/>
    </RelativeLayout>
    <View android:background="@color/color_ebebeb" android:layout_width="match_parent" android:layout_height="2dp"/>
    <androidx.recyclerview.widget.RecyclerView android:id="@+id/mp_faq_recycler_view" android:layout_width="match_parent" android:layout_height="match_parent"/>
</LinearLayout>
