<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_00aced"/>
            <corners android:topLeftRadius="@dimen/dimen_3dp" android:topRightRadius="@dimen/dimen_3dp" android:bottomLeftRadius="@dimen/dimen_3dp" android:bottomRightRadius="@dimen/dimen_3dp"/>
        </shape>
    </item>
    <item android:bottom="@dimen/dimen_3dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_dde5ed"/>
            <corners android:topLeftRadius="@dimen/dimen_3dp" android:topRightRadius="@dimen/dimen_3dp"/>
        </shape>
    </item>
    <item android:left="@dimen/dimen_1dp" android:top="@dimen/dimen_1dp" android:right="@dimen/dimen_1dp" android:bottom="@dimen/dimen_3dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_ffffff"/>
            <corners android:topLeftRadius="@dimen/dimen_3dp" android:topRightRadius="@dimen/dimen_3dp"/>
        </shape>
    </item>
</layer-list>
