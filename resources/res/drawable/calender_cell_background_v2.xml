<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:shape="rectangle">
    <item app:tsquare_state_range_first="true">
        <shape>
            <corners android:topLeftRadius="50dp" android:bottomLeftRadius="50dp"/>
            <solid android:color="@color/calendar_selected_day_bg"/>
        </shape>
    </item>
    <item app:tsquare_state_range_last="true">
        <shape>
            <corners android:topRightRadius="50dp" android:bottomRightRadius="50dp"/>
            <solid android:color="@color/calendar_selected_day_bg"/>
        </shape>
    </item>
    <item app:tsquare_state_range_middle="true">
        <shape>
            <solid android:color="@color/calendar_selected_range_bg"/>
            <stroke android:width="1dp" android:color="@color/calendar_selected_range_bg"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <corners android:topLeftRadius="50dp" android:topRightRadius="50dp" android:bottomLeftRadius="50dp" android:bottomRightRadius="50dp"/>
            <solid android:color="@color/calendar_selected_day_bg"/>
            <stroke android:width="1dp" android:color="@color/calendar_selected_day_bg"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:topLeftRadius="50dp" android:topRightRadius="50dp" android:bottomLeftRadius="50dp" android:bottomRightRadius="50dp"/>
            <solid android:color="@color/calendar_selected_day_bg"/>
            <stroke android:width="1dp" android:color="@color/calendar_selected_day_bg"/>
        </shape>
    </item>
    <item app:tsquare_state_current_month="false">
        <shape>
            <solid android:color="@color/white"/>
            <stroke android:width="1dp" android:color="@color/white"/>
        </shape>
    </item>
    <item app:tsquare_state_highlighted="true">
        <shape>
            <corners android:topLeftRadius="5dp" android:topRightRadius="5dp" android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp"/>
            <solid android:color="@color/calendar_highlighted_day_bg"/>
            <stroke android:width="1dp" android:color="@color/calendar_highlighted_day_bg"/>
        </shape>
    </item>
    <item app:tsquare_state_today="true">
        <shape>
            <corners android:topLeftRadius="40dp" android:topRightRadius="40dp" android:bottomLeftRadius="40dp" android:bottomRightRadius="40dp"/>
            <solid android:color="@color/calendar_bg"/>
            <stroke android:width="1dp" android:color="#ebebeb"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/calendar_bg"/>
            <stroke android:width="1dp" android:color="@color/calendar_bg"/>
        </shape>
    </item>
</selector>
