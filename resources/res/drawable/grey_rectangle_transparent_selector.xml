<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="@color/silver">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent"/>
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="@color/color_f5f8fa"/>
        </shape>
    </item>
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_f5f8fa"/>
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="@color/color_f5f8fa"/>
        </shape>
    </item>
</ripple>
