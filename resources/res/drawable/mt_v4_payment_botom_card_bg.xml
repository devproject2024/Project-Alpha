<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/dimen_1dp" android:color="#e8edf3"/>
            <corners android:topLeftRadius="12dp" android:topRightRadius="12dp" android:bottomLeftRadius="0dp" android:bottomRightRadius="@dimen/dimen_0dp"/>
            <solid android:color="#ffffff"/>
        </shape>
    </item>
    <item android:left="-2dp" android:top="-2dp" android:right="-2dp">
        <shape android:shape="line">
            <stroke android:width="1dp" android:color="#ffffff"/>
            <solid android:color="@null"/>
        </shape>
    </item>
</layer-list>
