<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#00b9f5"/>
            <corners android:topLeftRadius="3dp" android:topRightRadius="3dp" android:bottomLeftRadius="3dp" android:bottomRightRadius="3dp"/>
        </shape>
    </item>
    <item android:left="4dp" android:right="4dp">
        <shape android:shape="line">
            <stroke android:width="1dp" android:color="#00b9f5"/>
        </shape>
    </item>
</layer-list>
