<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <size android:height="1px" android:width="@dimen/dimen_50dp"/>
            <solid android:color="@color/seek_bar_background"/>
            <corners android:radius="2dp"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <size android:height="1px" android:width="@dimen/dimen_50dp"/>
                <solid android:color="@color/seek_bar_secondary_progress"/>
                <corners android:radius="2dp"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <size android:height="1px" android:width="@dimen/dimen_50dp"/>
                <solid android:color="@color/seek_bar_secondary_progress"/>
                <corners android:radius="2dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
