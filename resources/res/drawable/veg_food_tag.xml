<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_2dp"/>
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/green_text_color"/>
        </shape>
    </item>
    <item android:left="@dimen/dimen_3dp" android:top="@dimen/dimen_3dp" android:right="@dimen/dimen_3dp" android:bottom="@dimen/dimen_3dp">
        <shape android:shape="oval">
            <solid android:color="@color/green_text_color"/>
        </shape>
    </item>
</layer-list>
