<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:shape="rectangle">
    <item android:state_selected="true">
        <shape>
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="@color/calendar_selected_day_bg"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="@color/calendar_selected_day_bg"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="@color/gray"/>
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</selector>
