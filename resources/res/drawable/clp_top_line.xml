<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/clp_light_grey_border"/>
            <solid android:color="@color/clp_light_grey_border"/>
        </shape>
    </item>
    <item android:top="1dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/clp_white"/>
            <solid android:color="@color/clp_white"/>
        </shape>
    </item>
</layer-list>
