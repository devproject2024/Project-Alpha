<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_11dp"/>
            <solid android:color="@color/color_e8edf3"/>
            <size android:height="20dp" android:width="@dimen/dimen_33dp"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_11dp"/>
            <solid android:color="@color/color_00bcf1"/>
            <size android:height="20dp" android:width="@dimen/dimen_33dp"/>
        </shape>
    </item>
</selector>
