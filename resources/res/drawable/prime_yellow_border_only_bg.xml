<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#182233"/>
            <corners android:radius="6dp"/>
            <stroke android:width="1dp" android:color="@color/prime_bottom_pre_prime_bg"/>
            <padding android:left="@dimen/dimen_17dp" android:top="@dimen/dimen_11dp" android:right="@dimen/dimen_17dp" android:bottom="11dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#182233"/>
            <corners android:radius="6dp"/>
            <stroke android:width="1dp" android:color="@color/prime_bottom_pre_prime_bg"/>
            <padding android:left="@dimen/dimen_17dp" android:top="@dimen/dimen_11dp" android:right="@dimen/dimen_17dp" android:bottom="11dp"/>
        </shape>
    </item>
</selector>
