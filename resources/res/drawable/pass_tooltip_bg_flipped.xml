    <item android:top="18dp">
        <shape android:shape="rectangle">
            <solid android:color="#dde5ed"/>
            <corners android:radius="6dp"/>
        </shape>
    </item>
    <item android:left="1.5dp" android:top="19.5dp" android:right="1.5dp" android:bottom="1.5dp">
