<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/clp_light_grey_border"/>
            <size android:height="@dimen/dimen_3dp" android:width="@dimen/dimen_3dp"/>
            <corners android:radius="@dimen/dimen_3dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <layer-list>
                <item>
                    <color android:color="#00000000"/>
                </item>
                <item>
                    <shape>
                        <solid android:color="@color/review_orange"/>
                        <size android:height="@dimen/dimen_4dp" android:width="@dimen/dimen_4dp"/>
                        <corners android:radius="@dimen/dimen_4dp"/>
                    </shape>
                </item>
            </layer-list>
        </clip>
    </item>
</layer-list>
