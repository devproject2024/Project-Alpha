<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="oval">
            <gradient android:startColor="#ffffff" android:endColor="#e8e8e8" android:type="radial" android:gradientRadius="28dp"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="oval">
            <size android:height="35dp" android:width="35dp"/>
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>
